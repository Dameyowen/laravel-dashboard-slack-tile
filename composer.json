{
    "name": "dameyowen/laravel-dashboard-slack-tile",
    "description": "Laravel Dashboard tile listening for slack messages",
    "keywords": [
        "enflow",
        "dashboard",
        "tile",
        "slack-api"
    ],
    "homepage": "https://github.com/Dameyowen/laravel-dashboard-slack-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Damey",
            "email": "damey1999@live.nl"
        }
    ],
    "require": {
        "php": "^8.3",
        "lisennk/laravel-slack-events-api": "dev-laravelV7",
        "spatie/laravel-dashboard": "^1.0"
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Enflow\\Dashboard\\Tile\\Slack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Enflow\\Dashboard\\Tile\\Slack\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Enflow\\Dashboard\\Tile\\Slack\\SlackServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
